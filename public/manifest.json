{
  "manifest_version": 3,
  "name": "DataVerse",
  "description": "Extension for generating random data",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "icon-48x48.png",
      "64": "icon-64x64.png",
      "128": "icon-128x128.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "github.com@data-verse",
      "strict_min_version": "42.0"
    },
    "safari": {
      "strict_min_version": "14"
    }
  }
}
